{
    "name": "mudde/formgen4symfony",
    "description": "Tools for symfony to use FormGen",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olaf Mudde",
            "email": "olaf.mudde@xs4all.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Mudde\\Formgen4Symfony\\": "src" }
    },
    "require": {
        "php": ">=8.0.0",
        "symfony/http-kernel": "^5.2",
        "symfony/orm-pack": "^2.1",
        "symfony/console": "^5.2",
        "doctrine/annotations": "^1.13"
    },
    "require-dev": {
        "vimeo/psalm": "^4.19"
    },
    "scripts": {
        "post-install-cmd": [
            "npm install"
        ],
        "post-update-cmd": [
            "npm update"
        ]
    }
}
